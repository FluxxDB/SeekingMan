{
  "name": "template-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run prepare-game && run-in-roblox --place game.rbxlx --script ci-scripts/unit-testing.lua",
    "prepare-game": "rm -rd out && npm run compile && npm run build",
    "compile": "rbxts-build compile",
    "build": "rbxts-build build",
    "open": "rbxts-build open",
    "watch": "concurrently \"rbxtsc -w --verbose\" \"rojo serve\"",
    "start": "rbxts-build start && npm run watch",
    "sync": "rbxts-build sync",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix \"src/**/*.*\"",
    "fix:prettier": "prettier --write \"src/**/*.*\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"src/**/*.*\"",
    "lint:prettier": "prettier --check \"src/**/*.*\"",
    "deploy:staging": "npm run prepare-game && remodel run ci-scripts/deploy-game.lua staging",
    "deploy:prod": "npm run prepare-game && remodel run ci-scripts/deploy-game.lua production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rbxts/compiler-types": "^1.3.3-types.1",
    "@rbxts/types": "^1.0.593",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "concurrently": "^7.2.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-roblox-ts": "^0.0.32",
    "prettier": "^2.6.2",
    "rbxts-build": "^1.3.2",
    "rbxts-transformer-flamework": "^1.0.0-beta.10",
    "roblox-ts": "^1.3.3",
    "typescript": "^4.6.4"
  },
  "rbxts-build": {
    "rbxtscArgs": [
      "--verbose"
    ],
    "rojoBuildArgs": [
      "--output",
      "game.rbxlx"
    ],
    "syncLocation": "src/services.d.ts"
  },
  "dependencies": {
    "@flamework/components": "^1.0.0-beta.6",
    "@flamework/core": "^1.0.0-beta.4",
    "@flamework/networking": "^1.0.0-beta.6",
    "@rbxts/cmdr": "^1.8.4-ts.1",
    "@rbxts/fitumi": "^1.0.11",
    "@rbxts/flipper": "^2.0.1",
    "@rbxts/hook-bag": "^0.1.1",
    "@rbxts/inspect": "^1.0.1",
    "@rbxts/janitor": "^1.14.1-ts.0",
    "@rbxts/llama": "^1.1.1-ts.1",
    "@rbxts/log": "^0.6.3",
    "@rbxts/maid": "^1.0.0-ts.1",
    "@rbxts/make": "^1.0.6",
    "@rbxts/object-utils": "^1.0.4",
    "@rbxts/profileservice": "^1.4.1",
    "@rbxts/promise-character": "^1.0.3",
    "@rbxts/roact": "^1.4.2-ts.0",
    "@rbxts/roact-hooks": "^0.4.0-ts.0",
    "@rbxts/roact-rodux": "^0.2.2-ts.5",
    "@rbxts/rodux": "^3.0.0-ts.3",
    "@rbxts/services": "^1.2.0",
    "@rbxts/signal": "^1.0.3",
    "@rbxts/string-utils": "^1.0.3",
    "@rbxts/t": "^3.0.0",
    "@rbxts/testez": "^0.3.1-ts.7",
    "@rbxts/validate-tree": "^2.0.1",
    "@rbxts/visualize": "^1.1.1",
    "rbxts-transform-debug": "^1.1.0-ts4.5",
    "rbxts-transform-env": "^1.1.0-ts4.5"
  }
}
